                                                </a>
                                            </div>
                                            <!-- botao -->
                                        </div>
                                        <!-- botao_enc_noticia -->
                                        
                                </div>
                                <!-- bloco -->
                            </div>
                            <!-- imagem -->
                        
                        </div>
                        <!-- item  -->
                     <% }) %>
                  
                </div>
