<%- include ('partials/head.ejs') %>
    <div id="top"></div>

        <!-- main -->
    <main>
        <!-- inicio -->
        <% inicios.forEach(function(inicio) { %>
            <div class="inicio" style="background-image: url('/uploads/<%= inicio.imagem %>')">
                <div class="gradiente">
                    <div class="texto_inicio">
                        <h1 class="kanit-extrabold"><%= inicio.titulo %></h1>
                        <p><%= inicio.descricao %></p>
                    </div>
                </div>
            </div>
            <% }) %>
            <!-- inicio -->
            
            
            <!-- video -->
            <div class="destaque_principal">
                <div class="titulo_destaque">
                    <h1>Conheça nosso curso</h1>
                </div>
                <% videos.forEach(video => { %>
                <div class="conteudo_principal">
                    <video class="video_alinhamento" width="100%" height="100%" controls muted>
                        <source src="<%= video.url %>" type="video/mp4">
                        Seu navegador não suporta vídeos em HTML5.
                    </video>
                </div>
                <% }); %>
                
            
             </div>
        <!-- video -->
        
        
<!-- carrossel -->

        <div class="carrossel_noticias" ">
