                  <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
                  <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="1" aria-label="Slide 2"></button>
                  <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="2" aria-label="Slide 3"></button>
                </div>
                <!-- carrossel_inner -->
                <div class="carousel-inner">

                    <!-- item  -->
                    <% noticias.forEach((noticia, index) =>{ %>
                        <div class="carousel-item <%= index === 0 ? 'active' : '' %> " style="background-image: url('/arquivosNoticias/<%= noticia.imagem %>'); background-repeat: no-repeat; background-size: cover;" bac>
                            <!-- imagem -->
                            <div class="imagem_noticia1">
                                <!-- bloco -->
                                <div class="bloco_noticia">
                                    <div class="barra_colorida_bloco_noticia"></div>    
                                        <!-- texto -->
                                        <div class="texto_bloco_noticia">
                                            <h5><%= noticia.titulo %></h5>
                                            <p><%= noticia.conteudo %></p>
                                        </div>
                                        <!-- texto -->
                                            
                                            <!-- botao_enc_noticia -->
                                        <div class="botao_enc_noticia">
                                            <div class="espaco">
                                                
                                            </div>
                                            <!-- botao -->
                                            <div class="botao">
                                                <a href="/noticia/<%= noticia.slug %>">
                                                    <p>Saiba mais</p>
                                                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-arrow-right-square-fill" viewBox="0 0 16 16">
                                                        <path d="M0 14a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V2a2 2 0 0 0-2-2H2a2 2 0 0 0-2 2zm4.5-6.5h5.793L8.146 5.354a.5.5 0 1 1 .708-.708l3 3a.5.5 0 0 1 0 .708l-3 3a.5.5 0 0 1-.708-.708L10.293 8.5H4.5a.5.5 0 0 1 0-1"/>
